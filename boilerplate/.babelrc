{
  "presets": ["react-native"],
  "env": {
    "production": {
      "plugins": ["ignite-ignore-reactotron"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@common": "common/src",
          "@mobile": "./src",
          "@mobile/resources": "./resources"
        }
      }
    ]
  ],
  "sourceMaps": true
}
